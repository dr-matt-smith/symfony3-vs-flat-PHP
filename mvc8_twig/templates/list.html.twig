{% extends '_layout.html.twig' %}

{% block pageTitle %}List all Posts{% endblock %}

{% block content %}
<h1>List of Posts</h1>
<ul>
    {% for post in posts %}
        <li>
            <a href="/show?id={{ post.id }}">
                {{ post.title }}
            </a>
        </li>
    {% endfor %}
</ul>
{% endblock %}